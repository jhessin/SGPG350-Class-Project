<?xml version="1.0" encoding="utf-8"?>
<xs:schema xmlns:tns="TicTacToe.Service" elementFormDefault="qualified" targetNamespace="TicTacToe.Service" xmlns:xs="http://www.w3.org/2001/XMLSchema">
